{
    "name": "technopeak-monorepo",
    "private": true,
    "version": "1.0.0",
    "description": "Monorepo for TechnoPeak project",
    "scripts": {
      "dev": "pnpm -r run dev",
      "build": "pnpm -r run build",
      "preview": "cd frontend && vite preview",
      "lint": "pnpm -r run lint",
      "f:dev": "cd frontend && pnpm dev",
      "b:dev": "cd backend && pnpm dev",
      "f:build": "cd frontend && pnpm build",
      "b:build": "cd backend && pnpm build",
      "storybook": "cd frontend && storybook dev -p 6006",
      "test": "pnpm --filter frontend run test",
      "typecheck": "pnpm --filter frontend run typecheck"
    },
    "overrides": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
    "engines": {
      "node": ">=18.0.0",
      "pnpm": ">=9.0.0 <10.0.0"
    },
    "packageManager": "pnpm@9.6.0"
}